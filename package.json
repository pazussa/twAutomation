{
  "name": "autoloop-twilio-playwright",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "playwright:install": "playwright install",
    "pw": "playwright test",
    "pw:select": "playwright test",
    "pw:report": "playwright show-report",
    "report:pdf": "node scripts/export-report-to-pdf.mjs",
    "admin": "tsx src/admin/server.ts",
    "sync": "node scripts/sync-yml-to-data.mjs",
    "test:creation": "playwright test tests/creation-set.spec.ts --headed",
    "test:query": "playwright test tests/query-set.spec.ts --headed",
    "test:creation-headless": "playwright test tests/creation-set.spec.ts",
    "test:query-headless": "playwright test tests/query-set.spec.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "playwright": "^1.55.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "tsx": "^4.16.0"
  }
}